ude <stdio.h>
#include <math.h>
int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);
    int ans = sqrt(n);
    if (n == ans*ans)
    {
        printf("%d is a perfect square.\n", num);
    } 
    else
    {
        printf("%d is not a perfect square.\n", num);
    }

    return 0;
}
